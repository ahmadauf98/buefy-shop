    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^3.3.3",
    "axios": "^0.18.0",
    "bulma": "^0.9.1",
    "bulma-steps": "^2.2.1",
    "bulma-switch": "^2.0.0",
    "firebase": "^8.2.1",
    "lazysizes": "^5.3.0",
    "nuxt": "^2.14.12",
    "prettier": "^2.2.1",
    "slugify": "^1.4.6",
    "stylus": "^0.54.5",
