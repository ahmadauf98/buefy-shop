    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.9",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "pre-commit": "^1.2.2",
    "stylus-loader": "^3.0.1"
